#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="15" android:versionName="${version}" package="${package}.app">
    <application android:debuggable="true" android:icon="@drawable/icon" android:label="@string/app_name" android:name="${name}Application">
        <activity android:label="@string/app_name" android:name="Configure${name}Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="${name}Activity"/>
    </application>
    <uses-sdk android:minSdkVersion="14" android:targetSdkVersion="9"/>
</manifest>